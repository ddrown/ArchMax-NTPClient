#!/bin/sh

RUN=5-21-2020
STARTTIME=1589949339

#tail -100000 ${RUN}.log >${RUN}.log-zoom
tail -100000 ${RUN}.log-100M >${RUN}.log-zoom

for i in offset ptp; do
  echo $i
  if [ \! -f $i-${RUN}.log -o ${RUN}.log-zoom -nt $i-${RUN}.log ]; then
    grep -a ^$i: ${RUN}.log-zoom | ./parse-offset | awk '($1 > '$STARTTIME') { print }' >$i-${RUN}.log
  fi
  if [ \! -f $i-${RUN}.history -o $i-${RUN}.log -nt $i-${RUN}.history ]; then
    ./histogram 1 1 <$i-${RUN}.log >$i-${RUN}.history
  fi
done

for i in 10.1.2.128 10.1.2.198; do
  echo $i
  if [ \! -f $i-${RUN}.log -o ${RUN}.log-zoom -nt $i-${RUN}.log ]; then
    grep -a "^$i " ${RUN}.log-zoom | ./parse-ntp --adjust-rx-at=1590640140 | awk '($1 > '$STARTTIME') { print }' >$i-${RUN}.log
  fi
  if [ \! -f $i-${RUN}.history -o $i-${RUN}.log -nt $i-${RUN}.history ]; then
    ./histogram 4 1 <$i-${RUN}.log >$i-${RUN}.history
  fi
done

echo graphs
gnuplot <<EOF
set terminal png size 900,600
set xdata time
set timefmt "%s"
set output "offset-${RUN}.png"
set grid
set xlabel "Time"
set format x "%d-%H:%M"
set xtic rotate by -45 scale 0
set ylabel ""
set ytics format "%1.0f ns" nomirror
set title "offset between tim2/pps (after sync)"
set key top right box
plot \\
    'offset-${RUN}.log' using 1:2 title 'pps' with line
EOF

